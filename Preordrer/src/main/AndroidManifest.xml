<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.preorder.preorder" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".RestraintListActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SelectPickUpTimeActivity"
            android:label="@string/title_activity_select_pick_up_time"
            android:parentActivityName=".RestraintListActivity"
            android:theme="@style/AppTheme.NoActionBar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.preorder.preorder.RestraintListActivity" />
        </activity>
        <activity
            android:name=".GoodsSelectingActivity"
            android:label="@string/title_activity_goods_selecting"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".OrderConfirmationActivity" >
        </activity>
    </application>

</manifest>
